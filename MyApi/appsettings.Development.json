{
  "ConnectionStrings": {
    "SqlServer": "Data Source=.;Initial Catalog=MyApiDb;Integrated Security=true;TrustServerCertificate=True",
    "Elmah": "Data Source=.;Initial Catalog=Elmah;Integrated Security=true;TrustServerCertificate=True",
    "NLog": "Data Source=.;Initial Catalog=NLog;Integrated Security=true;TrustServerCertificate=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SiteSettings": {
    "ElmahPath": "/elmah-errors",
    "JwtSetting": {
      "SecretKey": "MySecretKeyForMyApiProject123456789",
      "Issuer": "MyWebsite",
      "Audience": "MyWebsite",
      "NotBeforeMinutes": "0",
      "ExpirationMinutes": "60"
    }
  }
}
